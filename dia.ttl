  rdfs:label "Facility" ;
  rdfs:comment "" ;
  rdf:value "A facility register" .

<https://test.uncefact.org/vocabulary/untp/dia/0/RegistryTypeCodeList#Land> a untp-dia:RegistryTypeCodeList ;
  rdfs:label "Land" ;
  rdfs:comment "" ;
  rdf:value "A land register" .

<https://test.uncefact.org/vocabulary/untp/dia/0/RegistryTypeCodeList#Product> a untp-dia:RegistryTypeCodeList ;
  rdfs:label "Product" ;
  rdfs:comment "" ;
  rdf:value "A product register" .

<https://test.uncefact.org/vocabulary/untp/dia/0/RegistryTypeCodeList#Trademark> a untp-dia:RegistryTypeCodeList ;
  rdfs:label "Trademark" ;
  rdfs:comment "" ;
  rdf:value "A trademark register" .


